    "symfony/console": "^6.0"
  },
  "require-dev": {
    "symfony/var-dumper": "^7.2",
    "phpunit/phpunit": "^10.0",
    "fakerphp/faker": "^1.24",
    "ext-gd": "*"
  },
  "autoload": {
