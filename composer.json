  "type": "library",
  "description": "Parse PHP projects into structured text files for LLM ingestion.",
  "license": "MIT",
  "version": "0.0.5",
  "authors": [
    {
      "name": "Bancu Adrian",
      "email": "adrian@bancu.ro"
    }
  ],
  "require": {
